{
  "name": "Marknotice - V2EX",
  "version": "0.0.6",
  "content_scripts": [ {
      "js": [ "jquery.js", "v2.js" ],
      "matches": [ "*://*.v2ex.com/*" ]
   } ],
  "description": "Get V2EX notification in the Bookmark Bar.",
  "permissions": [
    "bookmarks", "http://*.v2ex.com/*", "tabs"
  ],
  "icons": {
    "16": "assets/icon-16.png",
    "48": "assets/icon-48.png",
    "128": "assets/icon-128.png"
  },
  "background": {
    "page": "background.html"
  },
  "manifest_version": 6
}